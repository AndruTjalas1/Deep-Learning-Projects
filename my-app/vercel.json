{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://cst435react-production.up.railway.app/api/$1"
    },
    {
      "source": "/api/gan/(.*)",
      "destination": "https://gan-production.up.railway.app/$1"
    },
    {
